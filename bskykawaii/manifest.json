{
    "manifest_version": 3,
    "name": "Bluesky Kawaii",
    "version": "1.0",
    "description": "Modifies the BSKY_STORAGE key in local storage.",
    "permissions": [
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.bsky.app/*"],
        "js": ["content_script.js"]
      }
    ],
    "icons": {
      "128": "icon01.png"
    }
}
  